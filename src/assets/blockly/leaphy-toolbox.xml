                    </shadow>
                </value>
                <value name="LED_STRIP_DEMO_BLUE">
                    <shadow type="math_number">
                        <field name="NUM">
                            0
                        </field>
                    </shadow>
                </value>
            </block>
        </group>

        <group>
            <block type="leaphy_display_clear">
            </block>

            <block type="leaphy_display_set_text_size">
                <shadow type="math_number">
                    <field name="NUM">
